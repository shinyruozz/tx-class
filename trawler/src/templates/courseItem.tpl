<div class="course-item">
        <div class="course-img-wrap">
            <a href="{{href}}" class="course-lk">
                <img src="{{posterKey}}" alt="">
            </a>
        </div>
        <div class="course-info">
            <p class="course-tt">
                <a href="{{href}} " class="course-lk">
                    {{title}}
                </a>
            </p>

        </div>
        <div class="course-other">
            <span class='price'>
                {{price}}
            </span>
            <span class="apply-count">
                {{studentCount}}人报名
            </span>
        </div>

</div>